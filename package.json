{
  "name": "image-factory",
  "version": "0.8.3",
  "description": "Docker Image Factory",
  "keywords": [
    "docker",
    "image",
    "factory"
  ],
  "main": "./bin/image-factory",
  "bin": {
    "image-factory": "./bin/image-factory"
  },
  "directories": {
    "lib": "./lib"
  },
  "dependencies": {
    "async": "1.5.2",
    "aws-sdk": "2.0.0-rc13",
    "aws-swf": "4.0.0",
    "backoff": "2.4.1",
    "events": "~1.0.0",
    "formatter": "0.4.1",
    "hipchatter": "~0.2.0",
    "lodash": "3.2.0",
    "node-slack": "0.0.7",
    "nunjucks": "1.2.0",
    "optimist": "0.6.1",
    "request": "~2.53.0",
    "restify": "~2.7.0",
    "shortid": "2.1.3",
    "underscore.string": "3.0.3",
    "winston": "0.9.0",
    "wrench": "1.5.8",
    "z-schema": "2.4.6"
  },
  "devDependencies": {
    "chai": "~1.10.0",
    "sinon": "<2",
    "sinon-chai": "~2.6.0",
    "grunt": "1.0.3",
    "grunt-contrib-jshint": "2.1.0",
    "grunt-mocha-cov": "0.4.0",
    "mocha-term-cov-reporter": "0.2.3",
    "grunt-cli": "1.3.2",
    "grunt-shell": "3.0.1",
    "rewire": "4.0.1"
  },
  "scripts": {
    "start": "node bin/image-factory",
    "test": "./node_modules/.bin/grunt test",
    "blanket": {
      "pattern": [
        "image-factory/bin",
        "image-factory/lib"
      ]
    }
  },
  "engines": {
    "node": ">= 0.10.0"
  },
  "engineStrict": true,
  "author": {
    "name": "Mike Moulton",
    "email": "mike@meltmedia.com",
    "url": "http://www.meltmedia.com/"
  },
  "maintainer": [
    "Mike Moulton <mike@meltmedia.com>"
  ],
  "repository": {
    "type": "git",
    "url": "http://github.com/totem/image-factory.git"
  }
}
