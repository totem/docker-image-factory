{
  "name": "image-factory",
  "version": "0.8.2",
  "description": "Docker Image Factory",
  "keywords": [
    "docker",
    "image",
    "factory"
  ],
  "main": "./bin/image-factory",
  "bin": {
    "image-factory": "./bin/image-factory"
  },
  "directories": {
    "lib": "./lib"
  },
  "dependencies": {
    "async": "*",
    "aws-sdk": "2.0.0-rc13",
    "aws-swf": "4.0.0",
    "backoff": "*",
    "events": "~1.0.0",
    "formatter": "*",
    "hipchatter": "~0.2.0",
    "lodash": "*",
    "node-slack": "0.0.7",
    "nunjucks": "*",
    "optimist": "*",
    "request": "~2.53.0",
    "restify": "~2.7.0",
    "shortid": "*",
    "underscore.string": "*",
    "winston": "*",
    "wrench": "*",
    "z-schema": "2.4.6"
  },
  "devDependencies": {
    "chai": "~1.10.0",
    "sinon": "<2",
    "sinon-chai": "~2.6.0",
    "grunt": "*",
    "grunt-contrib-jshint": "*",
    "grunt-mocha-cov": "*",
    "mocha-term-cov-reporter": "*",
    "grunt-cli": "*",
    "grunt-shell": "*",
    "rewire": "*"
  },
  "scripts": {
    "start": "node bin/image-factory",
    "test": "./node_modules/.bin/grunt test",
    "blanket": {
      "pattern": [
        "image-factory/bin",
        "image-factory/lib"
      ]
    }
  },
  "engines": {
    "node": ">= 0.10.0"
  },
  "engineStrict": true,
  "author": {
    "name": "Mike Moulton",
    "email": "mike@meltmedia.com",
    "url": "http://www.meltmedia.com/"
  },
  "maintainer": [
    "Mike Moulton <mike@meltmedia.com>"
  ],
  "repository": {
    "type": "git",
    "url": "http://github.com/totem/image-factory.git"
  }
}
